    <link rel="stylesheet" href="https://lab.ring0.hk/css/svgMap.min.css">
    <style>
      .btn-rounded { border-radius: 10em; }
      #worldMapSliderLabels .slider-lower, #worldMapSliderLabels .slider-upper { font-size: 0.8em; }
      input[type=range].my-custom-range{-webkit-appearance:none;width:100%;margin:8px 0}input[type=range].my-custom-range:focus{outline:0}input[type=range].my-custom-range::-webkit-slider-runnable-track{width:100%;height:8px;cursor:pointer;box-shadow:.5px .5px 3px #888,0 0 .5px #959595;background:#ddd;border-radius:8px;border:.2px solid #010101}input[type=range].my-custom-range::-webkit-slider-thumb{box-shadow:2px 2px 4.1px #888,0 0 2px #959595;border:1px solid #ec407a;height:24px;width:12px;border-radius:12px;background:#ec407a;cursor:pointer;-webkit-appearance:none;margin-top:-8.2px}input[type=range].my-custom-range:focus::-webkit-slider-runnable-track{background:#e7e7e7}input[type=range].my-custom-range::-moz-range-track{width:100%;height:8px;cursor:pointer;box-shadow:.5px .5px 3px #888,0 0 .5px #959595;background:#ddd;border-radius:8px;border:.2px solid #010101}input[type=range].my-custom-range::-moz-range-thumb{box-shadow:2px 2px 4.1px #888,0 0 2px #959595;border:1px solid #ec407a;height:24px;width:12px;border-radius:12px;background:#ec407a;cursor:pointer}input[type=range].my-custom-range::-ms-track{width:100%;height:8px;cursor:pointer;background:0 0;border-color:transparent;color:transparent}input[type=range].my-custom-range::-ms-fill-lower{background:#d3d3d3;border:.2px solid #010101;border-radius:16px;box-shadow:.5px .5px 3px #888,0 0 .5px #959595}input[type=range].my-custom-range::-ms-fill-upper{background:#ddd;border:.2px solid #010101;border-radius:16px;box-shadow:.5px .5px 3px #888,0 0 .5px #959595}input[type=range].my-custom-range::-ms-thumb{box-shadow:2px 2px 4.1px #888,0 0 2px #959595;border:1px solid #ec407a;height:24px;width:12px;border-radius:12px;background:#ec407a;cursor:pointer;height:8px}input[type=range].my-custom-range:focus::-ms-fill-lower{background:#ddd}input[type=range].my-custom-range:focus::-ms-fill-upper{background:#e7e7e7}
    </style>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  </head>
