    <style>
      .btn-rounded { border-radius: 10em; }
      #worldMapSliderLabels .slider-lower, #worldMapSliderLabels .slider-upper { font-size: 0.8em; }
      /* Generated with range.css - http://danielstern.ca/range.css/#/ */
      input[type=range].my-custom-range{-webkit-appearance:none;width:100%;margin:5.9px 0}input[type=range].my-custom-range:focus{outline:0}input[type=range].my-custom-range::-webkit-slider-runnable-track{width:100%;height:12.2px;cursor:pointer;box-shadow:0 0 0 transparent,0 0 0 rgba(13,13,13,0);background:#eee;border-radius:6.5px;border:.2px solid #bbb}input[type=range].my-custom-range::-webkit-slider-thumb{box-shadow:1.5px 1.5px 6px #000,0 0 1.5px #0d0d0d;border:1px solid transparent;height:24px;width:12px;border-radius:7px;background:#ec407a;cursor:pointer;-webkit-appearance:none;margin-top:-6.1px}input[type=range].my-custom-range:focus::-webkit-slider-runnable-track{background:#fbfbfb}input[type=range].my-custom-range::-moz-range-track{width:100%;height:12.2px;cursor:pointer;box-shadow:0 0 0 transparent,0 0 0 rgba(13,13,13,0);background:#eee;border-radius:6.5px;border:.2px solid #bbb}input[type=range].my-custom-range::-moz-range-thumb{box-shadow:1.5px 1.5px 6px #000,0 0 1.5px #0d0d0d;border:1px solid transparent;height:24px;width:12px;border-radius:7px;background:#ec407a;cursor:pointer}input[type=range].my-custom-range::-ms-track{width:100%;height:12.2px;cursor:pointer;background:0 0;border-color:transparent;color:transparent}input[type=range].my-custom-range::-ms-fill-lower{background:#e1e1e1;border:.2px solid #bbb;border-radius:13px;box-shadow:0 0 0 transparent,0 0 0 rgba(13,13,13,0)}input[type=range].my-custom-range::-ms-fill-upper{background:#eee;border:.2px solid #bbb;border-radius:13px;box-shadow:0 0 0 transparent,0 0 0 rgba(13,13,13,0)}input[type=range].my-custom-range::-ms-thumb{box-shadow:1.5px 1.5px 6px #000,0 0 1.5px #0d0d0d;border:1px solid transparent;height:24px;width:12px;border-radius:7px;background:#ec407a;cursor:pointer;height:12.2px}input[type=range].my-custom-range:focus::-ms-fill-lower{background:#eee}input[type=range].my-custom-range:focus::-ms-fill-upper{background:#fbfbfb}
    </style>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
